# SSH commands
alias tower="ssh root@10.128.0.100"

# Navigate to windows drives
alias c="cd /mnt/c/"
alias d="cd /mnt/d/"
alias e="cd /mnt/e/"
alias f="cd /mnt/f/"

# A more detailed ls command
alias ll='ls -al'

# Replace ls for a better looking truncated ls
alias ls='ls -c'

# Open windows explorer at current location
alias explore='explorer.exe .'
